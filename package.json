    "eslint-config-xo": "^0.37.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^1.16.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "4.2.4"
  },
  "packageManager": "yarn@3.2.1"
